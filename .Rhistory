fit3 <- hw(train_aus, damped=TRUE, seasonal='additive')
fit4 <- hw(train_aus, damped=TRUE, seasonal='multiplicative')
accuracy(fit1, test_aus)
accuracy(fit2, test_aus)
accuracy(fit3, test_aus)
accuracy(fit4, test_aus)
autoplot(train_aus) +
autolayer(fit1, series="HW additive", PI=FALSE, size=1)+
autolayer(fit2, series="HW multiplicative", PI=FALSE, size=1)+
